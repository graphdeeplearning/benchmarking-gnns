{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MLP",
    "dataset": "TSP",
    
    "out_dir": "out/TSP_edge_classification/MLP/",
    
    "params": {
        "seed": 41,
        "epochs": 500,
        "batch_size": 64,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 1,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 132,
        "out_dim": 132,
        "readout": "mean",
        "gated": false,
        "in_feat_dropout": 0.0,
        "dropout": 0.0
    }
}